{
    "excess_spaces_removal" : {
	"class"    : "jp.supership.elasticsearch.plugin.queryparser.filter.string.ExcessSpacesRemovalFilter",
	"settings" : {
	    "removal_pattern" : "\\s+",
	    "substitution"    : " "
	}
    },
    "unicode_normalizing" : {
	"class"    : "jp.supership.elasticsearch.plugin.queryparser.filter.string.UnicodeNormalizingFilter",
	"settings" : {
	    "normalizer_form" : "NFKC"
	}
    }
}
